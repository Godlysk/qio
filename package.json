  "version": "0.0.0-development",
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.11.0",
    "cz-conventional-changelog": "^2.1.0",
    "mocha": "^6.0.2",
    "prettier": "^1.16.4",
    "semantic-release": "^15.13.3",
    "travis-deploy-once": "^5.0.11",
    "ts-node": "^8.0.3",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333",
    "typescript-tslint-plugin": "^0.3.1"
  }

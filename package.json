{
  "author": "Tushar Mathur <tusharmath@gmail.com>",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "description": "A typesafe functional module that solves practical IO problems for node and the browser.",
  "keywords": [
    "io",
    "functional",
    "typescript",
    "pure",
    "promise"
  ],
  "license": "ISC",
  "main": "index.js",
  "name": "fearless-io",
  "repository": {
    "type": "git",
    "url": "https://github.com/tusharmath/fearless-io.git"
  },
  "scripts": {
    "lint": "tslint --project .",
    "prepublishOnly": "tsc -d",
    "prettier": "git ls-files | grep '.ts$' | xargs prettier --write --config=.prettierrc",
    "semantic-release": "semantic-release",
    "test": "mocha --require=ts-node/register --watch-extensions ts test/*.ts",
    "travis-deploy-once": "travis-deploy-once --pro",
    "create-docs": "typedoc",
    "check-types": "typings-checker type-test/*",
    "typings-checker": "typings-checker",
    "example": "node example/src/Run"
  },
  "version": "0.0.0-development",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.0",
    "chai": "^4.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "mocha": "^6.1.4",
    "prettier": "^1.17.0",
    "semantic-release": "^15.13.3",
    "travis-deploy-once": "^5.0.11",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.5",
    "typescript-tslint-plugin": "^0.3.1",
    "typings-checker": "^2.0.0",
    "@types/promptly": "^1.1.28",
    "promptly": "^3.0.3"
  },
  "dependencies": {
    "dbl-linked-list-ds": "^2.0.0",
    "in-node": "^1.0.0",
    "ts-scheduler": "^4.1.2"
  }
}

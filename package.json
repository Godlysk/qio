{
  "private": true,
  "author": "Tushar Mathur <tusharmath@gmail.com>",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-lerna-changelog"
    }
  },
  "description": "A type-safe functional module that solves practical IO problems for node and the browser.",
  "keywords": [
    "io",
    "functional",
    "typescript",
    "pure",
    "promise"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/tusharmath/qio.git"
  },
  "scripts": {
    "lint": "tslint --project .",
    "prepublishOnly": "tsc -d",
    "prettier": "git ls-files | grep -E '.*\\.(ts|md|json)$' | xargs prettier --write --config=.prettierrc",
    "test": "mocha --opts=packages/chai/mocha.opts",
    "doc": "./scripts/docs.sh",
    "doc:typedoc": "yarn typedoc --plugin typedoc-plugin-markdown",
    "update-benchmarks": "packages/benchmarks/run.sh",
    "example": "node packages/example/guess-the-number/src/Run",
    "start": "yarn workspace @qio/website start",
    "doc:commit": "git add packages/docs && git commit -m 'docs(*): update docs \n\naffects: @qio/docs'",
    "website:commit": "git add packages/website && git commit -m 'website(*): update website \n\naffects: @qio/website'"
  },
  "version": "0.0.0-development",
  "devDependencies": {
    "cz-lerna-changelog": "^2.0.2",
    "lerna": "^3.18.4",
    "mocha": "^6.2.2",
    "prettier": "^1.19.1",
    "semantic-release": "^15.13.30",
    "ts-codemod": "^4.0.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.15.2",
    "typedoc-plugin-markdown": "^2.2.11",
    "typescript": "^3.7.2",
    "typescript-tslint-plugin": "^0.5.5"
  },
  "workspaces": [
    "packages/*"
  ],
  "name": "qio"
}

{
  "author": "Tushar Mathur <tusharmath@gmail.com>",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "description": "A type-safe functional module that solves practical IO problems for node and the browser.",
  "keywords": [
    "io",
    "functional",
    "typescript",
    "pure",
    "promise"
  ],
  "license": "ISC",
  "main": "packages/core/index.js",
  "name": "fearless-io",
  "repository": {
    "type": "git",
    "url": "https://github.com/tusharmath/fearless-io.git"
  },
  "scripts": {
    "lint": "tslint --project .",
    "prepublishOnly": "tsc -d",
    "prettier": "git ls-files | grep '.ts$' | xargs prettier --write --config=.prettierrc",
    "semantic-release": "semantic-release",
    "test": "mocha packages/**/*__tests__/**.js --opts=./mocha.opts",
    "travis-deploy-once": "travis-deploy-once --pro",
    "create-docs": "cd website; yarn build; cd ..; typedoc;",
    "update-benchmarks": "packages/benchmarks/run.sh",
    "check-types": "typings-checker --allow-expect-error type-test/*.ts",
    "typings-checker": "typings-checker",
    "example": "node packages/example/guess-the-number/src/Run"
  },
  "version": "0.0.0-development",
  "devDependencies": {
    "@tusharmathur/typedoc": "^1.0.0",
    "cz-conventional-changelog": "^3.0.2",
    "docusaurus": "^1.13.0",
    "lerna": "^3.8.0",
    "prettier": "^1.18.2",
    "semantic-release": "^15.13.24",
    "travis-deploy-once": "^5.0.11",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.4",
    "typescript-tslint-plugin": "^0.5.4",
    "typings-checker": "^2.0.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "private": true
}
